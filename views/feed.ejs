
<body>
<%- include('partials/header') -%>
<link rel="stylesheet" href = "css/feed.css"></link>
  <div class="container" id = "testi">
      <button class ="button_slide slide_down">Add Post</button>
      <button class = "button_slide slide_down">Sign Out</button>
    </div>
    <div class="row justify-content-center mt-4" id = "all-images-container">
      <ul class="row list-unstyled" id = "feed-image-width">
        <% for(var i=0; i<posts.length; i++) {%>
          <li class="col-4 justify-content-between mt-2">
            <a href="/post/<%= posts[i]._id%> " class ="images-feed">
      
              <img class = "heart" src = "https://em-content.zobj.net/source/skype/289/white-heart_1f90d.png"></img>
              <span class = "like-count">100</span>
              <img class="img-fluid" src="<%= posts[i].image%>"></img>
            </a>
          </li>
          
        <% } %>
        </ul>
    </div>
</body>
<%- include('partials/footer') -%>
